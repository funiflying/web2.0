    return g;
}
//查询字符串
function QueryString() {
    var url = location.search; //获取url中"?"符后的字串
    var qs = new Object();
    if (url.indexOf("?") != -1) {
        var str = url.substr(1);
        strs = str.split("&");
        for(var i = 0; i < strs.length; i ++) {
            qs[strs[i].split("=")[0]]=decodeURIComponent(strs[i].split("=")[1]);
        }
    }
    return qs;
}
