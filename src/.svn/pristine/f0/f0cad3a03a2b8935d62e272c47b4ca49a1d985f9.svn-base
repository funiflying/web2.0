    for (var i = 0, plen = e1.length; i < plen; i++) {
        if (e1[i]['id'] == parseInt(c1)) {
            f1 = e1[i]['city'];
            break
        }
    }
    var c2 = obj.data('id');
    var e2 = f1;
    var f2 = [];
    for (var i = 0, plen = e2.length; i < plen; i++) {
        if (e2[i]['id'] == parseInt(c2)) {
            f2.push(e2[i]);
        }
    }

    var e = f2;
    var f = [];
    var g = '';
    for (var i = 0, plen = e.length; i < plen; i++) {
        if (e[i]['city']) {
            f=e[i]['city'];
        }
    }

    for (var j = 0, clen = f.length; j < clen; j++) {
        g += '<a data-level="1" data-id="' + f[j]['id'] + '" data-name="' + f[j]['name'] + '" title="' + f[j]['name'] + '">' + f[j]['name'] + '</a>'
    }

