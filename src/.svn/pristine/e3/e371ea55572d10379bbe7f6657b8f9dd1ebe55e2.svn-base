/*
*btn
*/
.tui-btn {
    border: none;
    font-size: 14px;
    font-weight: 400;
    line-height: 1.4;
    border-radius: 4px;
    padding: 11px 15px;
    -webkit-transition: border .25s linear,color .25s linear,background-color .25s linear;
    -moz-transition: border .25s linear,color .25s linear,background-color .25s linear;
    transition: border .25s linear,color .25s linear,background-color .25s linear;
    -webkit-font-smoothing: subpixel-antialiased;
}
.btn-secondary {
    color: #FFF;
    background-color: #ff6a00;
}
.btn-filter, .btn-link:focus, .btn-link:hover, .btn-outline-action, .btn-outline-inverse, .btn-outline-primary, .btn-outline-secondary {
    background-color: transparent;
}
.btn-outline-primary.active, .btn-outline-primary:active, .btn-outline-primary:focus, .btn-outline-primary:hover {
    color: #ff6a00;
}
.btn.disabled, .btn[disabled], fieldset[disabled] .btn{
    cursor: default;
}
.tui-btn.disabled, .tui-btn[disabled], fieldset[disabled] .tui-btn {
   /* background-color: inherit;*/
    color: rgba(255,255,255,.75);
    opacity: .7;
    -webkit-opacity: .7;
    -moz-opacity: .7;
    -khtml-opacity: .7;
    filter: alpha(opacity=70);
}

.btn-orange {
    color: #fff;
    background-color: #ff6a00;
    border-color: #ff6a00;
    border-radius: 0;
}
.btn-orange.active, .btn-orange:active {
    background: #FF560F;
}
.btn-orange.active, .btn-orange:active, .btn-orange:focus, .btn-orange:hover {
    color: #FFF;
    background-color: #FF4D26;
    border-color: #FF4D26;
}
.btn-orange:link,.btn-orange:visited .btn-orange:hover,.btn-orange:active,{
    color: #fff;
    background-color: #ff5903;
    border-color: #ff5903;
}
